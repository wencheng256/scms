<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Index</servlet-name>
    <servlet-class>com.wencheng.web.ui.Index</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Register</servlet-name>
    <servlet-class>com.wencheng.web.ui.Register</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegisterAction</servlet-name>
    <servlet-class>com.wencheng.web.controller.RegisterAction</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckName</servlet-name>
    <servlet-class>com.wencheng.web.controller.CheckName</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegisterCheckUsername</servlet-name>
    <servlet-class>com.wencheng.web.controller.RegisterCheckUsername</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegisterCheckNumber</servlet-name>
    <servlet-class>com.wencheng.web.controller.RegisterCheckNumber</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterAction</servlet-name>
    <url-pattern>/registeraction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckName</servlet-name>
    <url-pattern>/register/checkname</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterCheckUsername</servlet-name>
    <url-pattern>/register/checkusername</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterCheckNumber</servlet-name>
    <url-pattern>/register/checknumber</url-pattern>
  </servlet-mapping>

</web-app>